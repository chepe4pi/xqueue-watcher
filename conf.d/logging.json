{
    "loggers": {
            "": {
                "handlers": ["console"],
                "level": "ERROR"
            },
            "requests": {
                "level": "ERROR",
                "handlers": ["console"]
            }
    },
    "version": 1,
    "handlers": {
        "console": {
            "class": "logging.StreamHandler",
            "level": "ERROR",
            "formatter": "standard"
        }
    },
    "formatters": {
        "raw": {
            "format": "%(message)s"
        },
        "standard": {
            "format": "%(asctime)s %(levelname)s %(process)d [%(name)s] %(filename)s:%(lineno)d - %(message)s"
        }
    }
}

